{
  "metadata": [
    {
      "src": [
        {
          "src": "../Packages/com.nickmaltbie.StateMachineUnity",
          "files": [
            "**/*.cs"
          ]
        },
        {
          "src": "../Assets/Samples",
          "files": [
            "**/*.cs"
          ]
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "filterConfig.yml",
      "dest": "api",
      "version": "latest"
    }
  ],
  "build": {
    "template": [
      "default",
      "templates/custom"
    ],
    "globalMetadata": {
      "_appTitle": "State Machine Unity Documentation",
      "_appFooter": "State Machine Unity Documentation",
      "_enableSearch": true,
      "_version": "v1.1.4",
      "_versionList": "v1.1.4,v1.1.3,v1.1.2,v1.1.1,v1.1.0,v1.0.1,v1.0.0,v0.1.0,v0.0.1,v0.0.0"
    },
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "src": "api",
        "files": [
          "toc.yml",
          "*.yml"
        ],
        "dest": "api"
      },
      {
        "src": "changelog",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "changelog"
      },
      {
        "src": "manual",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "manual"
      }
    ],
    "overwrite": [
      {
        "src": "../Packages/com.nickmaltbie.StateMachineUnity",
        "files": [
          "**/*.cs"
        ]
      }
    ],
    "resource": [
      {
        "src": ".",
        "files": [
          "resources/**/*",
          "Demo/**/*",
          "LICENSE.txt"
        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://nickmaltbie.com/StateMachineUnity/docs",
      "changefreq": "weekly",
      "fileOptions": {
        "api/*": {
          "changefreq": "daily"
        }
      }
    },
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml",
      "https://nickmaltbie.com/TestUtilsUnity/docs/xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "D:\\Projects\\StateMachineUnity/_site/v1.1.4"
  }
}
